  },
  "homepage": "https://github.com/kenny1997/kennyshop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.0.12",
    "nodemon": "^2.0.15"
  }
}
